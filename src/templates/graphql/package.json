{
  "name": "screen-projection",
  "version": "0.0.0",
  "description": "Screen Projection:Web builder with drag and drop",
  "scripts": {
    "start": "vite",
    "dev": "concurrently \"yarn start\" \"yarn graphql:codegen\"",
    "build": "tsc && vite build",
    "test": "jest",
    "serve": "vite preview",
    "prepare": "husky install",
    "commit": "commit",
    "format": "prettier --write --ignore-unknown .",
    "lint": "eslint --cache ./src",
    "analyze": "source-map-explorer 'dist/assets/*.js'",
    "gen:theme-typings": "chakra-cli tokens ./src/theme/index.ts",
    "postinstall": "yarn gen:theme-typings",
    "graphql:codegen": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@craftjs/core": "^0.2.0-beta.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "axios": "^0.24.0",
    "framer-motion": "^5",
    "graphql": "^16.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-icons": "^4.3.1",
    "react-location": "^3.3.0",
    "react-query": "^3.34.7"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.6.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/prompt-cli": "^15.0.0",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@graphql-codegen/typescript-react-query": "^3.3.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.14",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-react": "^1.1.3",
    "autoprefixer": "^10.4.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.2",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2",
    "source-map-explorer": "^2.5.2",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.2",
    "vite-tsconfig-paths": "^3.3.17"
  }
}
